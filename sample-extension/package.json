{
  "name": "sb-sample-extension",
  "description": "Extension Sample",
  "version": "0.0.0",
  "main": "dist/index.js",
  "keywords": [],
  "author": "Unbounce - Content Foundations",
  "license": "UNLICENSED",
  "browserslist": [
    "last 2 versions"
  ],
  "scripts": {
    "check-types": "tsc --noEmit",
    "lint": "npm run lint:prettier && npm run lint:eslint",
    "test": "jest",
    "lint:eslint": "eslint src --ext .ts,.tsx",
    "lint:prettier": "prettier --check \"src/**/*.{js,jsx,ts,tsx}\"",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "build": "rm -rf dist && webpack --env production",
    "start": "concurrently --names 'CLIENT,SERVER' --prefix-colors 'yellow,blue' --kill-others \"npm run client\" \"npm run serve\"",
    "client": "rm -rf dist && npm run compile-manifest && webpack --watch",
    "serve": "node serve-local-files.js",
    "init-repo-settings": "sh bin/init-repo-settings.sh",
    "compile-manifest": "tsc --project tsconfig.manifest.json"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=7"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.14.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@testing-library/dom": "^8.11.1",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/react-hooks": "^7.0.0",
    "@testing-library/user-event": "^13.5.0",
    "@trivago/prettier-plugin-sort-imports": "^2.0.2",
    "@types/jest": "^27.0.2",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.2",
    "@types/react-test-renderer": "^17.0.1",
    "@types/styled-components": "^5.1.20",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/typescript-estree": "^4.25.0",
    "babel-loader": "^8.0.6",
    "concurrently": "^7.5.0",
    "cors": "^2.8.5",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-unicorn": "^32.0.1",
    "express": "^4.17.1",
    "jest": "^27.3.1",
    "nodemon": "3.0.1",
    "prettier": "^2.3.0",
    "tsconfig-paths-jest": "^0.0.1",
    "typescript": "4.6.4",
    "webpack": "^5.59.0",
    "webpack-cli": "^4.9.1",
    "whatwg-fetch": "^3.6.2"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "smart-builder-sdk": "https://app-assets-unbounce-com.s3.amazonaws.com/content-smart-builder-apps/smart-builder-sdk/smart-builder-sdk-1.0.0-beta.90.tgz",
    "styled-components": "^5.3.3",
    "semver": "^7.5.4",
    "simple-update-notifier": "^2.0.0"
  }
}
